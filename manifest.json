{
  "manifest_version": 2,
  "name": "Pinput",
  "version": "0.2.3",
  "description": "Light-weight chrome extension for Pinboard.",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "page": "/src/html/background.html"
  },
  "browser_action": {
    "default_icon": "/src/icon/icon_32.png",
    "default_title": "Pinput",
    "default_popup": "/src/html/popup.html"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+B"
      },
      "description": "Open popup window."
    }
  },
  "offline_enabled": false,
  "author": "1000ch"
}